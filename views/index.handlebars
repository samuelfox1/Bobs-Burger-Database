<main>
  <div class="container">
    <div class="row">
      <div class="col s6 offset-s2 menue">
        <div class="row place-order">
          <h2>Place an Order</h2>
          <form class="order-form">
            <div class="form-group">
              {{!-- <label for="order-name">Burger Name:</label> --}}
              <input type="text" id="order-name" name="name">
              <button type="submit">Order Burger</button>
            </div>
          </form>
        </div>
        <h2>Order UP!</h2>

        <ul>
          {{#each burgers}}
          {{#unless devoured}}
          {{> burgers/burger-block devoured=true}}
          {{/unless}}
          {{/each}}
        </ul>

      </div>
    </div>
  </div>
</main>
<h2>Order History</h2>

<ul>
  {{#each burgers}}
  {{#if devoured}}
  {{> burgers/burger-block devoured=false}}
  {{/if}}
  {{/each}}
</ul>